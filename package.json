    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-project-relative-import": "^2.0.0",
    "babel-register": "^6.11.6",
